	"version": "0.1.0",
	"description": "CLI command to start a server using @cfware/fastify-test-helper",
	"main": "index.js",
	"scripts": {
		"release": "standard-version --sign",
		"test": "xo && nyc ava -v"
	},
	"engines": {
		"node": ">=8"
	},
	"author": "Corey Farrell",
	"license": "MIT",
