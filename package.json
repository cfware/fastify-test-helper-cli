		"import-from": "^2.1.0",
		"pkg-dir": "^4.0.0"
	},
	"devDependencies": {
		"@cfware/fastify-test-helper": "^0.2.0",
		"ava": "^1.2.1",
		"nyc": "^13.3.0",
		"p-event": "^4.0.0",
		"standard-version": "^5.0.0",
		"xo": "^0.24.0"
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
