	],
	"bin": {
		"cfware-fastify-test-helper": "./bin.js"
	},
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/fastify-test-helper-cli.git"
	},
	"bugs": {
		"url": "https://github.com/cfware/fastify-test-helper-cli/issues"
	},
	"homepage": "https://github.com/cfware/fastify-test-helper-cli#readme",
	"dependencies": {
